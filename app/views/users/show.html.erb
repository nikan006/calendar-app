<% if user_signed_in? %>
    <%= link_to "新規イベント追加", new_event_path %>

    <%= month_calendar events: @events do |date, events| %>
      <%= date.day %>

      <% events.each do |event| %>
        <div>
          <%= link_to event.title, event %>
        </div>
      <% end %>
    <% end %>
<% end %>