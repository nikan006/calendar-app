<h1>テスト</h1>

<%= form_with(model: @event, local: true) do |form| %>

  <div class="title">
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>

  <div class="time">
    <%= form.label :start_time %>
    <%= form.datetime_select :start_time %>
  </div>

  <div class="time">
    <%= form.label :end_time %>
    <%= form.datetime_select :end_time %>
  </div>

  <div class="content">
    <%= form.label :text %>
    <%= form.text_field :text %>
  </div>


  <div class="submit">
    <%= form.submit %>
  </div>

<% end %>